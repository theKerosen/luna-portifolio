{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "angular",
    "installCommand": "npm install --legacy-peer-deps",
    "headers": [
        {
            "source": "/(.*).(jpg|jpeg|png|svg|ico|webp|js|css|woff|woff2)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}